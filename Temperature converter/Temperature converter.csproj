<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <RootNamespace>basictempconverter</RootNamespace>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <PackageId>basictempconverter</PackageId>
    <Title>simple temperature converter</Title>
    <Version>1.0.0</Version>
    <Authors>Stanley</Authors>
    <Company>Motion studio</Company>
    <Product>simple temperature converter</Product>
    <Description>This is a basic library that converts Celcius to Fahrenheit and vice versa</Description>
    <PackageReadmeFile>Temperature Converter.md</PackageReadmeFile>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <AssemblyName>basictempconverter</AssemblyName>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\..\..\..\Downloads\Temperature Converter.md">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
  </ItemGroup>

</Project>
